  "main": "./dist/svelte-device-info.js",
  "module": "./dist/svelte-device-info.esm.js",
  "svelte": "./dist/svelte-device-info.esm.js",
  "exports": {
    ".": {
      "require": "./dist/svelte-device-info.js",
