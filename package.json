  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "agadoo": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.6",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.2"
  }
